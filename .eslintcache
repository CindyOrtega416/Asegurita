lumn;\r\n  border-radius: 19px;\r\n  background-color: #fff;\r\n  box-shadow: 0 0 2px rgba(15, 15, 15, 0.28);\r\n  position: relative;\r\n  overflow: hidden;\r\n`;\r\n\r\nconst TopContainer = styled.div`\r\n  width: 100%;\r\n  height: 250px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-end;\r\n  padding: 0 1.8em;\r\n  padding-bottom: 5em;\r\n`;\r\n\r\nconst BackDrop = styled(motion.div)`\r\n  width: 160%;\r\n  height: 550px;\r\n  position: absolute;\r\n  display: flex;\r\n  flex-direction: column;\r\n  border-radius: 50%;\r\n  transform: rotate(60deg);\r\n  top: -290px;\r\n  left: -70px;\r\n  background: rgb(122,158,159);\r\n  background: linear-gradient(90deg, rgba(122,158,159,1) 0%, rgba(253,29,29,0.4417344173441734) 50%, rgba(227,227,172,1) 100%);\r\n  );\r\n`;\r\n\r\nconst HeaderContainer = styled.div`\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n`;\r\n\r\nconst HeaderText = styled.h2`\r\n  font-size: 36px;\r\n  font-weight: 600;\r\n  line-height: 2.60;\r\n  color: #fff;\r\n  z-index: 10;\r\n  margin: 0;\r\n\r\n`;\r\n\r\nconst SmallText = styled.h5`\r\n  color: #fff;\r\n  font-weight: 500;\r\n  font-size: 11px;\r\n  z-index: 10;\r\n  margin: 0;\r\n  margin-top: 7px;\r\n`;\r\n\r\nconst InnerContainer = styled.div`\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  padding: 0 1.8em;\r\n`;\r\n\r\nconst backdropVariants = {\r\n  expanded: {\r\n    width: \"233%\",\r\n    height: \"1050px\",\r\n    borderRadius: \"20%\",\r\n    transform: \"rotate(60deg)\",\r\n  },\r\n  collapsed: {\r\n    width: \"160%\",\r\n    height: \"550px\",\r\n    borderRadius: \"50%\",\r\n    transform: \"rotate(60deg)\",\r\n  },\r\n};\r\n\r\nconst expandingTransition = {\r\n  type: \"spring\",\r\n  duration: 2.3,\r\n  stiffness: 30,\r\n};\r\n\r\nexport function AccountBox(props) {\r\n  const [isExpanded, setExpanded] = useState(false);\r\n  const [active, setActive] = useState(\"signin\");\r\n\r\n  const playExpandingAnimation = () => {\r\n    setExpanded(true);\r\n    setTimeout(() => {\r\n      setExpanded(false);\r\n    }, expandingTransition.duration * 1000 - 1500);\r\n  };\r\n\r\n  const switchToSignup = () => {\r\n    playExpandingAnimation();\r\n    setTimeout(() => {\r\n      setActive(\"signup\");\r\n    }, 400);\r\n  };\r\n\r\n  const switchToSignin = () => {\r\n    playExpandingAnimation();\r\n    setTimeout(() => {\r\n      setActive(\"signin\");\r\n    }, 400);\r\n  };\r\n\r\n  const contextValue = { switchToSignup, switchToSignin };\r\n\r\n  return (\r\n    <AccountContext.Provider value={contextValue}>\r\n      <BoxContainer>\r\n        <TopContainer>\r\n          <BackDrop\r\n            initial={false}\r\n            animate={isExpanded ? \"expanded\" : \"collapsed\"}\r\n            variants={backdropVariants}\r\n            transition={expandingTransition}\r\n          />\r\n          {active === \"signin\" && (\r\n            <HeaderContainer>\r\n              <HeaderText>Segurita</HeaderText>\r\n            </HeaderContainer>\r\n          )}\r\n          {active === \"signup\" && (\r\n            <HeaderContainer>\r\n              <HeaderText>Registrarse</HeaderText>\r\n            </HeaderContainer>\r\n          )}\r\n        </TopContainer>\r\n        <InnerContainer>\r\n          {active === \"signin\" && <LoginForm />}\r\n          {active === \"signup\" && <SignupForm />}\r\n        </InnerContainer>\r\n      </BoxContainer>\r\n    </AccountContext.Provider>\r\n  );\r\n}\r\n","C:\\Users\\Usuario\\WebstormProjects\\Asegurita\\src\\components\\accountBox\\accountContext.js",[],"C:\\Users\\Usuario\\WebstormProjects\\Asegurita\\src\\components\\accountBox\\loginForm.jsx",[],"C:\\Users\\Usuario\\WebstormProjects\\Asegurita\\src\\components\\accountBox\\signupForm.jsx",[],"C:\\Users\\Usuario\\WebstormProjects\\Asegurita\\src\\components\\accountBox\\common.jsx",[],"C:\\Users\\Usuario\\WebstormProjects\\Asegurita\\src\\components\\marginer\\index.jsx",[],"C: